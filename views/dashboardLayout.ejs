<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Rewards App Dashboard</title>

    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <!--STYLES-->
    <link rel="stylesheet" href="/styles/importer.css">
    <!--STYLES END-->
    <!-- Angular Material CSS now available via Google CDN; version 0.10 used here -->
    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/angular_material/0.10.0/angular-material.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body ng-app="dashboardApp" ng-controller="AppCtrl" ng-cloak>
    <section layout="row" layout-fill>
        <md-sidenav layout="column" class="md-primary md-sidenav-left md-whiteframe-z2" md-component-id="left" md-is-locked-open="$mdMedia('gt-md')">
            <md-toolbar class="md-primary">
                <div class="md-toolbar-tools">
                    <span>Main Menu</span>
                </div>
            </md-toolbar>
            <md-list flex ng-controller="SideMenuCtrl">
                <md-list-item ng-repeat="menuItem in menuItems" ng-click="menuClicked(menuItem.action)" class="md-icon-">
                    <p>{{ menuItem.title }}</p>
                    <md-icon md-font-library="material-icons">{{ menuItem.icon }}</md-icon>
                </md-list-item>
            </md-list>
            </md-content>
        </md-sidenav>
        <div layout="column" flex>
            <md-toolbar >
              <div class="md-toolbar-tools">
                <h2>
                  <a href="/dashboard">{{ pageProperties.pageTitle }}</a>
                </h2>
                <span flex></span>
                <md-button href="/logout" class="md-primary md-raised">Log out</md-button>
              </div>
            </md-toolbar>
            <div flex>
                <%- body %>
            </div>
        </div>
    </section>

    <!-- Angular Material Dependencies -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular-animate.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular-aria.min.js"></script>


    <!-- Angular Material Javascript now available via Google CDN; version 0.10 used here -->
    <script src="https://ajax.googleapis.com/ajax/libs/angular_material/0.10.0/angular-material.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.15/angular-ui-router.min.js"></script>

    <!--SCRIPTS-->
    <script src="/js/dependencies/sails.io.js"></script>
    <script src="/js/app/home/homeApp.js"></script>
    <script src="/js/app/home/controllers/AppCtrl.js"></script>
    <script src="/js/app/home/controllers/HomeCtrl.js"></script>
    <script src="/js/app/home/controllers/ProviderDialogCtrl.js"></script>
    <script src="/js/app/home/controllers/RegisterCtrl.js"></script>
    <script src="/js/app/home/controllers/SigninCtrl.js"></script>
    <script src="/js/app/home/services/notificationService.js"></script>
    <script src="/js/app/home/services/providerService.js"></script>
    <script src="/js/app/home/services/registrationService.js"></script>
    <script src="/js/app/home/services/securityService.js"></script>
    <script src="/js/app/dashboard/dashboardApp.js"></script>
    <script src="/js/app/dashboard/controllers/AboutViewCtrl.js"></script>
    <script src="/js/app/dashboard/controllers/AddMemberViewCtrl.js"></script>
    <script src="/js/app/dashboard/controllers/AppCtrl.js"></script>
    <script src="/js/app/dashboard/controllers/FamilyViewCtrl.js"></script>
    <script src="/js/app/dashboard/controllers/MemberMenuCtrl.js"></script>
    <script src="/js/app/dashboard/controllers/MemberViewCtrl.js"></script>
    <script src="/js/app/dashboard/controllers/SettingsViewCtrl.js"></script>
    <script src="/js/app/dashboard/controllers/SideMenuCtrl.js"></script>
    <script src="/js/app/dashboard/directives/raCardMenu.js"></script>
    <script src="/js/app/dashboard/services/Member.js"></script>
    <script src="/js/app/dashboard/services/cardService.js"></script>
    <script src="/js/app/dashboard/services/configService.js"></script>
    <script src="/js/app/dashboard/services/familyService.js"></script>
    <script src="/js/app/dashboard/services/goalsService.js"></script>
    <script src="/js/app/dashboard/services/pagePropertiesService.js"></script>
    <!--SCRIPTS END-->
</body>
</html>
